<Window x:Class="Store.Employees"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Employees" Height="300" Width="442">
    <Grid Margin="0,0,-281,-6">
        <ListBox Name="lbEmployees" HorizontalAlignment="Left" Height="131" Margin="35,44,0,0" VerticalAlignment="Top" Width="87"/>
        <TextBox Name="tbFirstName" HorizontalAlignment="Left" Height="23" Margin="239,47,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="tbLastName" HorizontalAlignment="Left" Height="23" Margin="239,75,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="tbUnit" HorizontalAlignment="Left" Height="23" Margin="239,103,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="First Name" HorizontalAlignment="Left" Margin="140,44,0,0" VerticalAlignment="Top" Width="85"/>
        <Label Content="Second Name" HorizontalAlignment="Left" Margin="140,73,0,0" VerticalAlignment="Top" Width="85"/>
        <Label Content="Unit" HorizontalAlignment="Left" Margin="140,100,0,0" VerticalAlignment="Top" Width="85"/>
        <Button Name="btnAddEmployee" Content="Add" HorizontalAlignment="Left" Margin="284,131,0,0" VerticalAlignment="Top" Width="75" Click="btnAddEmployee_Click"/>
        <ListBox x:Name="lbEmployees_Copy" HorizontalAlignment="Left" Height="131" Margin="381,28,0,0" VerticalAlignment="Top" Width="87"/>
        <TextBox x:Name="tbFirstName_Copy" HorizontalAlignment="Left" Height="23" Margin="585,31,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbLastName_Copy" HorizontalAlignment="Left" Height="23" Margin="585,59,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbUnit_Copy" HorizontalAlignment="Left" Height="23" Margin="585,87,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label Content="First Name" HorizontalAlignment="Left" Margin="486,28,0,0" VerticalAlignment="Top" Width="85"/>
        <Label Content="Second Name" HorizontalAlignment="Left" Margin="486,57,0,0" VerticalAlignment="Top" Width="85"/>
        <Label Content="Unit" HorizontalAlignment="Left" Margin="486,84,0,0" VerticalAlignment="Top" Width="85"/>
        <Button x:Name="btnAddEmployee_Copy" Content="Add" HorizontalAlignment="Left" Margin="630,115,0,0" VerticalAlignment="Top" Width="75" Click="btnAddEmployee_Click"/>

    </Grid>
</Window>
